<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD//Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
<properties resource="org/hws/dblink/credenciales.properties"/>

  
  <typeAliases>
	<typeAlias alias="panel" type="org.hws.repo.Panel"/>	
  </typeAliases> 
  

<environments default="development">
<environment id="development">
<transactionManager type="JDBC"/>	

<dataSource type="POOLED">
<property name="driver"	 		value="${driver}"/>
<property name="url" 			value="${puente}${server}:${port}/${db}${ssl}"/>
<property name="username" 		value="${usuario}"/>
<property name="password" 		value="${pws}"/>



</dataSource>
</environment>
</environments>

<mappers>
	<mapper resource="org/hws/repo/Panel.xml"/>
</mappers>
 
</configuration>